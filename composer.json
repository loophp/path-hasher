{
    "name": "loophp/path-hasher",
    "description": "Library to serialize a filesystem path",
    "license": "EUPL-1.2",
    "type": "library",
    "keywords": [
        "nar",
        "serializer",
        "nix"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "Pol Dellaiera",
            "email": "pol.dellaiera@protonmail.com",
            "homepage": "https://not-a-number.io",
            "role": "author"
        }
    ],
    "homepage": "https://github.com/loophp/path-hasher",
    "support": {
        "issues": "https://github.com/loophp/path-hasher/issues",
        "source": "https://github.com/loophp/path-hasher"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/drupol"
        }
    ],
    "require": {
        "php": "^8.1",
        "loophp/iterators": "^3.2"
    },
    "require-dev": {
        "ext-pcov": "*",
        "ergebnis/composer-normalize": "^2.48",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpro/grumphp": "^2",
        "phpunit/php-code-coverage": "^10",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Loophp\\PathHasher\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "phpro/grumphp": true
        },
        "sort-packages": true
    },
    "scripts": {
        "grumphp": "./vendor/bin/grumphp run"
    }
}
